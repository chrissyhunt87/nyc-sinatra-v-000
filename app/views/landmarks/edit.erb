<h1>Edit <%=@landmark.name%></h1>
<form action="/landmarks/<%=@landmark.id%>" method="post">
  <input id="hidden" type="hidden" name="_method" value="patch">
  <p>Landmark Name: <input id="name" type="text" name="landmark[name]" value="<%=@landmark.name%>"></p>
  <p>Year Completed: <input id="year_completed" type="text" name="landmark[year_completed]" value="<%=@landmark.year_completed.to_s%>"></p>
  <input type="submit" name="submit" value="Edit Landmark">
</form>
